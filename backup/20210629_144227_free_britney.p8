pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
function create_britney()
	p={x=6,y=4,sprite=1}
end

function britney_movement()
	newx=p.x
	newy=p.y
	if(btnp(➡️)) newx+=1
	if(btnp(⬅️)) newx-=1
	if(btnp(⬆️)) newy-=1
	if(btnp(⬇️)) newy+=1
	
	if (newx>10) then	
		p.x=127
	elseif (newx<0) then
		p.x=0
	else
		p.x=newx 
	end	
	--if (newy>127
	--p.x=mid(0,newx,127)
	--p.y=mid(0,newy,127)
end

function draw_britney()
	spr(p.sprite,p.x*8,p.y*8)
end

