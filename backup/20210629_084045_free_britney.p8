pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
--variables globales
btn_speed=1


function _init()
	x=60
	y=30
end

function _update()
	if(btn(➡️)) x+=1
	if(btn(⬅️)) x-=1
	if(btn(⬆️)) y-=1
	if(btn(⬇️)) y+=1
	
	if(spr(1) >127) then
	btn_speed=-1
	end
end

function _draw()
 cls()
	map(0,0,0,0)
	spr(1,x,y)

end
